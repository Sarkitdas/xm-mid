#include <iostream>
#include<bits.h>

using namespace std;

int main()
{
    string a;
    cin>>a;
    int lngth=a.size();

    if(a[0]==47 && a[1]==42)
    {
        if(a[lngth-2]==42 && a[lngth-1]==47)
        {
            cout<<"Yes it comment"<<endl;
        }
        else
        {
            cout<<"NO it is not comment"<<endl;
        }

    }
    else
    {
        cout<<"NO it is not comment"<<endl;
    }

return 0;
}

